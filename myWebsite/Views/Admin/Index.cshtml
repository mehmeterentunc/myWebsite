@using myWebsite.Models.classes
@model List<Web>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table table-bordered mt-3">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Update</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.id</td>
            <td>@x.name</td>
            <td><a href="/Admin/bringWeb/@x.id" class="btn btn-primary">Update</a> </td>
        </tr>

    }

</table>

